@{
    ViewBag.Title = "LandingPage";
    Layout = "~/Views/Shared/_LayoutNON.cshtml";

}
@Styles.Render("~/Themes/2013/stylesheets/StyleSheet1.css")
<script type="text/javascript">
    var message = '@ViewBag.message';
    if (message != '') {
        $("#ManMade_Alert_Message").html(message);
        ManMade_Alert_Message_Window.data("kendoWindow").center().open();
    } 
</script>
<div class="Wrapper">
    <h1 id="Header"><a href="https://sellerportal.newegg.com.tw/" class="logo" title="Back landing page">Back landing page</a></h1>
    <div class="content">
        <div class="createAccount">
            <h2>Not a Seller on Newegg Marketplace? </h2>
            <p>Apply today to sell your items on Newegg Marketplace.</p>
            <div class="buttonBar">
                <a href="http://promotions.newegg.com/marketplace/merchants/apply.html" class="button btnA"><span><em>Apply Now</em></span></a>
            </div>
            
            </div>
        <div class="login">
            @*<h2>I'm a Seller.</h2> *@
            <form name="loginForm" action="/Account/LandingPage" method="post">
                <ul>
                    <li class="caption">
						<strong class="icon iconID">Login ID</strong>
						<span class="exampleEmail">(example: JohnDoe@Newegg.com)</span>
                    </li>
                    <li class="form">
                        <div class="boxInputA">
                            <span class="circleL"></span><span class="circleR"></span>
                            <input type="text" class="input" value="" name="UserEmail" id="UserEmail">
                        </div>
                    </li>
                    <li class="caption"><strong class="icon iconPassword">Password</strong></li>
                    <li class="form">
                        <div class="boxInputA">
                            <span class="circleL"></span><span class="circleR"></span>
                            <input type="password" class="input" value="" name="UserPwd" id="UserPwd">
                            
                        </div>
                    </li>
                   
                   <input type="hidden" name="LoginAppendix" value="7R0yeUCOqQvhlpdMco0HfoiH1VeLAs64l4d/r9wz+BIAvIstVsNkeImUk2ITBUKT" />
                    <li class="caption"><strong class="icon iconAccountType">Account Type</strong></li>
                    <li class="form">
                        <div class="boxSelection">
                            <select style="width:300px;height:30px;padding:5px" name="VendorSeller">
                                <option value="V">I am a vendor</option>
                                <option value="S">I am a seller</option>
                            </select>
                        </div>
                    </li>
                    
                    <li class="remember">
                        <label><input type="checkbox" id="IsRemember"  name="IsRemember"> Remember my login ID</label>
                        <a href="/Account/ResetPassword">Forgot your password?</a></li>
                </ul>
                <div class="buttonBar">
					<input type="image" src="https://ssl-images.newegg.com/WebResource/Themes/2005/Nest/SellerPortal/button/loginbutton.png" alt="Login">
                </div>
              </form>
        </div>
        <div class="clear"></div>
    </div>
    <div id="Footer">
    	<a onclick="javascript:window.open(&#39;https://sellerportal.newegg.com/Pages/Account/PolicyAndAgreement.aspx&#39;,  &#39;newwindow&#39;, &#39;height=600,width=900, left=0,top=0,toolbar =no, menubar=no, scrollbars=no, resizable=no, location=no, status=no&#39;)" href="javascript:void(null);">Policy &amp; Agreement</a> | <a href="http://www.newegg.com/HelpInfo/PrivacySecurity.aspx">Privacy Policy</a> | © 2013 Newegg Inc. All rights reserved.
    </div>
</div>

