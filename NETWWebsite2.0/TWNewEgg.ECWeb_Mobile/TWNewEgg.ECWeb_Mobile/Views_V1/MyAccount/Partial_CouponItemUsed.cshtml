@using TWNewEgg.Models.ViewModels.Redeem
@model Coupon
@{
    ViewBag.Title = "CouponItem";
}

                <div class="head-bar orangebg2">@(Model.title)</div>
                            <ul class="list-unstyled">
                                <li>品項 : @(Model.ordcode)</li>
                                <li>使用日期 : @(Model.validend.ToString("yyyy/MM/dd HH:mm:ss"))</li>
                                <li>金額 : @(String.Format("{0:#}元",Model.value))</li>
                                <br/>
                                <li>適用範圍 : @(Model.categories)</li>
                                <hr/>
                                <li>狀態 : @(ViewBag.CouponStatus)</li>
                            </ul>
