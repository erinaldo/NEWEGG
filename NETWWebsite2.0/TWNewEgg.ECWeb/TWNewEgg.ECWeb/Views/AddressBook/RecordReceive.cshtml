                        </td>
                    </tr>
                    <tr></tr>
                </table>
            </div>
        </div>
    </body>
</html>
<script type="text/javascript">
    @*設定預設的收件人*@
    var SettingDefaultAddress = parseInt("@defaultSettingID");
    if(SettingDefaultAddress != 0) {
        $("img[id *=addressbook_default_]").css("display", "none"); @*全部隱藏*@
        $("img[id *=addressbook_default_" + SettingDefaultAddress + "]").attr("style", "cursor:pointer");
        $("input[id *=addressbook_choose_" + SettingDefaultAddress + "]").css("display", "none");
    }
    @*限定只能填入數字*@
    $("input[numberrule='true']").keypress(function (e) {
        if ((e.shiftKey && e.keyCode == 45) || e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
