            url: "/Addressbook/ModifyAddressBook",
            contentType: "application/json;charset=utf-8",
            data: jason,
            success: function (res){
                if(res.indexOf('[Error]') >= 0){
                    var Error = res.split('[Error]')[1];
                    alert(Error);
                    return;
                }
                else if(res.indexOf('[Message]') >= 0){
                    var Message = res.split('[Message]')[1];
                    alert(Message);
                    window.location.reload();
                }
                else{
                    alert("修改錯誤，請確認資料填寫正確");
                }
            }
        });
    }
</script>
