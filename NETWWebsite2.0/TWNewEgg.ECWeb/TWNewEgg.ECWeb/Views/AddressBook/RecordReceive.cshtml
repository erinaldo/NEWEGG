@model List<TWNewEgg.DB.TWSQLDB.Models.Addressbook>
@{
    int defaultSettingID = 0;
    if (Model.Count > 0)
    {
        List<TWNewEgg.DB.TWSQLDB.Models.Addressbook> addressbooks = Model.Where(x=>x.DefaultSetting == 1).ToList();
        if(addressbooks.Count > 1) {
            defaultSettingID = addressbooks[0].ID;
        }
        else if(addressbooks.Count == 1)
        {
            defaultSettingID = addressbooks[0].ID;
        }
        else {
            defaultSettingID = Model[0].ID;
        }
    }
}
